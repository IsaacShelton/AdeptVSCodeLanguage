{
    "editor.insertSpaces": false,
    "tslint.enable": true,
    "typescript.tsc.autoDetect": "off",
    "typescript.preferences.quoteStyle": "single",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "cSpell.words": [
        "Struct",
        "Unflushed",
        "autofilled",
        "cstring",
        "malloc"
    ],
    "adeptLanguageServer.trace.server": "verbose"
}